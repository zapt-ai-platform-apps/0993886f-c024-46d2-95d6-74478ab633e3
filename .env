# Environment Variables

# VITE_PUBLIC_APP_ID: Unique identifier for the app (provided by ZAPT)
VITE_PUBLIC_APP_ID=your_app_id_here

# VITE_PUBLIC_APP_ENV: Application environment (e.g., development, production)
VITE_PUBLIC_APP_ENV=development

# VITE_PUBLIC_SENTRY_DSN: DSN for Sentry error tracking
VITE_PUBLIC_SENTRY_DSN=your_sentry_dsn_here

# VITE_PUBLIC_UMAMI_WEBSITE_ID: Umami analytics website ID
VITE_PUBLIC_UMAMI_WEBSITE_ID=your_umami_website_id_here

# COCKROACH_DB_URL: Connection string for CockroachDB
COCKROACH_DB_URL=your_cockroach_db_url_here